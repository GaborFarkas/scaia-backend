LAYER
    PROJECTION
        AUTO
    END
    NAME "222P21_R1"
    TYPE RASTER
    STATUS ON
    DATA {layername}
    CLASSITEM "[pixel]"
    CLASS
        NAME "class1"
        EXPRESSION ([pixel] >= 0 AND [pixel] <= 1)
        STYLE
            COLORRANGE 182 184 190 15 220 207
            DATARANGE 0 1
        END
    END
    METADATA
        "wms_title" "222P21_R1"
    END
END # LAYER
