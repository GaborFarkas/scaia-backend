LAYER
    PROJECTION
        AUTO
    END
    NAME "222P22_R2"
    TYPE RASTER
    STATUS ON
    DATA {layername}
    CLASSITEM "[pixel]"
    CLASS
        NAME "class1"
        EXPRESSION ([pixel] >= 0 AND [pixel] <= 1)
        STYLE
            COLORRANGE 196 196 196 8 48 107
            DATARANGE 0 1
        END
    END
    METADATA
        "wms_title" "222P22_R2"
    END
END # LAYER
