LAYER
    PROJECTION
        AUTO
    END
    NAME "222P24_R2"
    TYPE RASTER
    STATUS ON
    DATA {layername}
    CLASSITEM "[pixel]"
    CLASS
        NAME "class1"
        EXPRESSION ([pixel] >= 0 AND [pixel] <= 1)
        STYLE
            COLORRANGE 255 245 240 103 0 13
            DATARANGE 0 1
        END
    END
    METADATA
        "wms_title" "222P24_R2"
    END
END # LAYER
